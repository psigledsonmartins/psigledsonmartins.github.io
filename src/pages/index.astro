  .clip-svg {
    clip-path: url("#blob1");
  }
  .sombra-1 {
    bottom: 5%;
    left: -20%;
  }
  .titulo {
    font-size: 2rem;
    font-weight: 600;
  }
  .subtitulo {
    font-size: 1.4rem;
    font-weight: 600;
  }

